{
  "name": "@nartallax/picgen_webgui",
  "version": "1.0.0",
  "description": "A web GUI to wrap arbitrary picture generating program with.",
  "author": "Nartallax",
  "license": "0BSD",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:client & npm run dev:server & wait",
    "dev:client": "./node_modules/.bin/parcel serve --target client --no-cache --no-hmr --port 24658",
    "dev:server": "npm run build:server && npm run dev:server:run",
    "dev:server:run": "node ./dist/server/server_main.js --http-root ./target/ --config config.example.json --http-root-url 'http://localhost:24658'",
    "build:server": "./node_modules/.bin/parcel build --target server --no-cache",
    "typecheck": "cd src && ../node_modules/typescript/bin/tsc --noEmit && cd - > /dev/null"
  },
  "devDependencies": {
    "@nartallax/package_syncer": "^1.0.4",
    "@parcel/plugin": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@types/cls-hooked": "^4.3.3",
    "@types/node": "^17.0.5",
    "@types/pngjs": "^6.0.1",
    "@types/probe-image-size": "^7.2.0",
    "@types/shell-quote": "^1.7.1",
    "@types/sqlite3": "^3.1.8",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "buffer": "^5.7.1",
    "eslint": "^8.5.0",
    "parcel": "^2.8.3",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@nartallax/ribcage": "^1.1.5",
    "@nartallax/ribcage-validation": "^1.0.1",
    "cls-hooked": "^4.2.2",
    "discord.js": "^14.3.0",
    "pngjs": "^6.0.0",
    "probe-image-size": "^7.2.3",
    "shell-quote": "^1.7.3",
    "sqlite3": "^5.1.6",
    "ws": "^8.8.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9"
  },
  "targets": {
    "server": {
      "source": "src/server/server_main.ts",
      "context": "node",
      "optimize": false,
      "outputFormat": "esmodule"
    },
    "client": {
      "source": "static/index.html"
    }
  }
}
